{
  "name": "create-cronos",
  "version": "0.2.6",
  "description": "Generate a project with Vite, Express, Tauri, Next.js, Astro and other technologies with a single command.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cronosjs-labs/create-cronos"
  },
  "homepage": "cronosjs.dev/",
  "scripts": {
    "ci": "act",
    "build": "rollup -c",
    "install-global": "npm install -g .",
    "remove-ignore": "rm -r ignore/",
    "create-ignore": "mkdir ignore",
    "build-install": "turbo build install-global --concurrency=1",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --config .prettierrc 'src/**/*.ts' --write"
  },
  "bin": {
    "create-cronos": "./bin/app.js"
  },
  "keywords": [
    "create-cronos",
    "project generator",
    "cronosjs-labs"
  ],
  "author": "Erick Medel",
  "license": "MIT",
  "dependencies": {
    "@inquirer/checkbox": "^1.4.0",
    "@inquirer/prompts": "^5.0.0",
    "@inquirer/select": "^1.3.0",
    "cross-spawn": "^7.0.3",
    "esbuild": "0.19.5",
    "express-ts-transform": "^0.0.13",
    "fs-extra": "^11.1.1",
    "inquirer": "^9.2.11",
    "inquirer-autocomplete-standalone": "^0.8.1",
    "node-persist": "^4.0.1",
    "prompts": "^2.4.2",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/node": "^20.11.30",
    "@types/yargs": "^17.0.32",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "prettier": "^3.1.0",
    "rollup": "^4.12.0",
    "turbo": "^1.12.4"
  }
}