{
  "name": "cronos.express",
  "version": "0.0.0",
  "main": "./dist/index.js",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "test": "vitest",
    "test:ui": "vitest --ui",
    "build": "esbuild ./src/index.js --bundle --platform=node --target=node14 --outfile=./dist/index.js",
    "dev": "nodemon src/index.js",
    "start": "node dist/index.js",
    "lint": "eslint -c .eslintrc.json ./src --ext .js",
    "lint:fix": "eslint -c .eslintrc.json ./src --ext .js --fix",
    "format": "prettier --write \"src/**/*.js\""
  },
  "dependencies": {
    "express": "4.18.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@swc/cli": "0.1.62",
    "@swc/core": "1.3.82",
    "@swc/helpers": "0.5.3",
    "@vitest/ui": "^1.2.1",
    "dotenv": "^16.3.1",
    "esbuild": "^0.19.12",
    "eslint": "8.51.0",
    "nodemon": "3.0.1",
    "prettier": "^3.2.4",
    "regenerator-runtime": "0.14.0",
    "supertest": "^6.3.4",
    "vitest": "^1.2.1"
  }
}
